                &lt;span class=&quot;code-keyword&quot;&gt;xmlns:ext&lt;/span&gt;=&lt;span class=&quot;code-quote&quot;&gt;&quot;urn:viacomcablelabs:md:xsd:ext:3.0&quot;&lt;/span&gt;&amp;gt;
